{
  "name": "fluent-stream",
  "description": "Fluent Stream Coding Challenge",
  "version": "1.0.0",
  "main": "dist/index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "clean": "rimraf dist/*",
    "copy-assets": "ts-node tools/copyAssets",
    "build": "npm-run-all clean tsc copy-assets",
    "preserve": "npm run build",
    "serve": "cross-env NODE_ENV=development concurrently \"tsc --watch\" \"nodemon -q dist/index.js\"",
    "prestart": "npm run build",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsc": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KC135Q/fluent-stream.git"
  },
  "keywords": [],
  "author": "Dan Kaltenbaugh <d.a.kaltenbaugh@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KC135Q/fluent-stream/issues"
  },
  "homepage": "https://github.com/KC135Q/fluent-stream#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "helmet": "^4.4.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.11",
    "@types/fs-extra": "^9.0.10",
    "@types/node": "^14.14.37",
    "@types/shelljs": "^0.8.8",
    "concurrently": "^6.0.1",
    "dotenv": "^8.2.0",
    "fs-extra": "^9.1.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3"
  }
}
